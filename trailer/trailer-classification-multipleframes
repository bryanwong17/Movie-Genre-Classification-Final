h % 60))\n","\n","  vc.set(cv2.CAP_PROP_POS_AVI_RATIO, 0.5)\n","  total_imgs = 100\n","  total_frames = vid_length # sampling rate 1 frame/sec\n","  frame_idx = int((total_frames - total_imgs) // 2) if total_imgs <= total_frames else 0\n","\n","  for i in tqdm(range(frame_idx, frame_idx + total_imgs, 1)):\n","    if frame_idx >= total_frames:\n","      continue\n","    else:\n","      vc.set(cv2.CAP_PROP_POS_MSEC, frame_idx * 1000)\n","      success, this_img = vc.read()\n","      if not success:\n","        print(\"Image capture failed.\")\n","        break\n","      cv2.imwrite('drive/MyDrive/zt-trailer/trailer-frames/' + mn + str(frame_idx) + \".jpg\", this_img)\n","    frame_idx += 1\n","\n","  return string_vid_length"],"execution_count":6,"outputs":[]},{"cell_type":"code","metadata":{"id":"Ufh1tfe2X2DK","executionInfo":{"status":"ok","timestamp":1623968349198,"user_tz":-480,"elapsed":5,"user":{"displayName":"Michelle Soen","photoUrl":"https://lh3.googleusercontent.com/a-/AOh14GhG74W72MpjNnvPlJwsYxmR0Hi0UCbBWyPEhKMH=s64","userId":"03820532445770256937"}}},"source":["train_data = pd.read_csv('drive/MyDrive/zt-trailer/train_data.csv').drop(\"Unnamed: 0\", axis=1)\n","test_data = pd.read_csv('drive/MyDrive/zt-trailer/test_data.csv').drop(\"Unnamed: 0\", axis=1)"],"execution_count":7,"outputs":[]},{"cell_type":"code","metadata":{"id":"k45c5rmCaVB7","colab":{"base_uri":"https://localhost:8080/"},"executionInfo":{"status":"ok","timestamp":1623970479856,"user_tz":-480,"elapsed":2130411,"user":{"displayName":"Michelle Soen","photoUrl":"https://lh3.googleusercontent.com/a-/AOh14GhG74W72MpjNnvPlJwsYxmR0Hi0UCbBWyPEhKMH=s64","userId":"03820532445770256937"}},"outputId":"57f61fc0-d506-486e-a9a8-fd49d8f4ad05"},"source":["train_data['img_data'] = train_data['movie'].apply(get_imgs)\n","test_data['img_data'] = test_data['movie'].apply(get_imgs)"],"execution_count":8,"outputs":[{"output_type":"stream","text":["drive/MyDrive/zt-trailer/trailer-dataset/100earthquake.mp4\n"],"name